    <!-- add a Team Services feed -->
    <!-- <add key="pates" value="https://pate90.pkgs.visualstudio.com/_packaging/ACMCommon/nuget/v3/" /> -->
    <add key="patesRegs" registry="https://pate90.pkgs.visualstudio.com/_packaging/ACMCommon/npm/registry" always-auth="true" />
    <add key="pates" name="ACMCommon" value="https://pate90.pkgs.visualstudio.com/_packaging/ACMCommon/nuget/v3/index.json" />
    <!-- also get packages from the NuGet Gallery -->
    <add key="nuget.org" value="https://api.nuget.org/v3/" />
  </packageSources>
</configuration>